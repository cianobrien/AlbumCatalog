<h1>AudioPhiles</h1>

<table>
  <tbody>
    <% @profiles.each do |profile| %>
        <td>
        <%= link_to image_tag(profile.profile_picture_url, size: "100x150", class: 'list_image'), profile %>
      </td>
	<td>
	  <dl>
	    <pre>
	      <h3><%= profile.name %></h3>
	    </pre>
	  </dl>
	</td>
</table>
    <% end %>
  </tbody>
</table>


